{
  "name": "mdmnv/filament-database-viewer",
  "description": "Reusable Filament page for browsing and editing relational database tables.",
  "type": "library",
  "keywords": [
    "laravel",
    "database",
    "viewer"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "filament/filament": "^4.0",
    "livewire/livewire": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Mdmnv\\FilamentDatabaseViewer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mdmnv\\FilamentDatabaseViewer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mdmnv\\FilamentDatabaseViewer\\DatabaseBrowserServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
